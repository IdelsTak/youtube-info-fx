<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>


<DialogPane prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.idelstak.youtubeinfofx.ui.controllers.APIKeyDialogController">
  <buttonTypes>
    <ButtonType fx:constant="CANCEL" />
    <ButtonType fx:constant="APPLY" />
  </buttonTypes>
  <content>
    <VBox spacing="6.0">
      <children>
        <Label text="API Key" />
        <TextField />
      </children>
    </VBox>
  </content>
  <header>
    <VBox>
      <children>
        <VBox>
          <children>
            <Label text="Create or select a project in the API Console. Complete the following tasks in the Google Developers API Console for your project:" wrapText="true" />
          </children>
          <VBox.margin>
            <Insets />
          </VBox.margin>
          <padding>
            <Insets bottom="6.0" left="6.0" right="6.0" top="6.0" />
          </padding>
        </VBox>
      </children>
    </VBox>
  </header>
  <padding>
    <Insets bottom="6.0" left="6.0" right="6.0" top="6.0" />
  </padding>
</DialogPane>
